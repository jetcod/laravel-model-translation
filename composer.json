{
    "name": "jetcod/laravel-translation",
    "description": "Translation Manager Package for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "Translation",
        "localization"
    ],
    "authors": [
        {
            "name": "Hamid Ghorashi",
            "email": "h.ghorashi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/config": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "autoload": {
        "psr-4": {
            "Jetcod\\Laravel\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetcod\\Laravel\\Translation\\Test\\": "tests/"
        }
    },
    "scripts": {
        "phpcs-fix": "./vendor/bin/php-cs-fixer fix ",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=-1 tests/",
        "test": "./vendor/bin/phpunit --testdox"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Jetcod\\Laravel\\Translation\\Providers\\TranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
